#!/usr/bin/python
import sys, os, os.path as path
import itertools

TYPES = ['.jar', '-sources.jar', '-javadoc.jar', '.pom']
HASHES = ['.sha1', '.md5', '']

def mresolve(spec):
    org, art, ver = spec.split(':')
    orgd = org.replace('.', '/')
    rbase = "https://repo1.maven.org/maven2"
    ubase = path.join(rbase, orgd, art, ver, art + "-" + ver)
    return list(map(lambda x: ''.join(x), itertools.product([ubase], TYPES, HASHES)))

if __name__ == "__main__":
    for x in itertools.chain.from_iterable(map(mresolve, sys.argv[1:])):
        print x
