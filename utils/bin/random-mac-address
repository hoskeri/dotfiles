#!/usr/bin/env python
from __future__ import print_function
import random

def random_mac():
    r = map(lambda x: random.randrange(0, 256), range(0, 6))
    r[0] &= 0xfc
    r[0] |= 0x2
    return r

def mac_printable(r):
    return ":".join(map(lambda x: hex(x).replace('0x', '').rjust(2, '0'), r))

if __name__ == "__main__":
    print(mac_printable(random_mac()))
